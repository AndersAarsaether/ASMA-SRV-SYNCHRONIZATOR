{
    "name": "asma-srv-synchronizator",
    "version": "0.0.0",
    "description": "",
    "main": "./dist/server.js",
    "type": "module",
    "license": "Proprietary",
    "scripts": {
        "dev": "tsx --inspect --watch  ./src/server.ts",
        "build": "npx esbuild src/server.ts  --bundle --outfile=dist/server.mjs --format=esm --platform=node --banner:js=\"const { require, __filename,__dirname } = await (async () => {\n\tconst { createRequire } = await import('node:module');\n\tconst { fileURLToPath } = await import('node:url');\n\treturn {\n\t\trequire: createRequire(import.meta.url),\n\t\t__filename: fileURLToPath(import.meta.url),\n\t\t__dirname: fileURLToPath(new URL('.', import.meta.url)),\n\t};\n})();\"",
        "run": "node ./dist/server.cjs",
        "eslint": "eslint \"./**/*.{js,ts}\"",
        "eslint:fix": "eslint --fix \"./**/*.{js,ts}\"",
        "prettier": "prettier --write \"./**/*.{js,ts}\""
    },
    "dependencies": {
        "axios": "^1.5.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "cookie": "^0.5.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "fastify": "^4.24.3",
        "fs": "0.0.1-security",
        "graphql": "^16.8.1",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "lodash-es": "^4.17.21",
        "multer": "1.4.5-lts.1",
        "node-cache": "^5.1.2",
        "node-fetch": "^3.3.2",
        "retry": "^0.13.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@genql/cli": "^6.2.1",
        "@types/bcryptjs": "^2.4.4",
        "@types/cookie": "^0.5.2",
        "@types/express": "^4.17.18",
        "@types/express-serve-static-core": "^4.17.37",
        "@types/jsonwebtoken": "^9.0.3",
        "@types/lodash-es": "^4.17.9",
        "@types/multer": "^1.4.9",
        "@types/node": "^20.8.4",
        "@types/node-fetch": "^2.6.6",
        "@types/retry": "^0.12.5",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "asma-types": "^1.6.4",
        "esbuild": "^0.19.4",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "esm": "^3.2.25",
        "import": "^0.0.6",
        "tsx": "^3.13.0",
        "typescript": "^5.2.2"
    },
    "imports": {
        "@EnvConfigs": "EnvConfigs"
    },
    "keywords": [],
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    }
}
